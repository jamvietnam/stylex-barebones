      #
      #   • Create the file - news.html
      #   • Then add the menu item and reference to the .yml file.
      #     - title: News
      #       path: news
      #
      #########-->

      <div class="wrapper">
        {% block hero %}{% endblock %}      <!-- See .html for the block content -->
        {% block content %}{% endblock %}   <!-- See .html for the block content -->
